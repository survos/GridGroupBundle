{
    "name": "survos/grid-group-bundle",
    "type": "symfony-bundle",
    "description": "Handle groups of CSV as a spreadsheet",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "twig/twig": "^3.4",
        "league/csv": "^9.0",
        "illuminate/collections": "^10.9",
        "symfony/string": "^6.2",
        "symfony/form": "^6.2"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html public/coverage",
        "tests": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "Survos\\GridGroupBundle\\Tests\\": "tests/",
            "Survos\\GridGroupBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "symfony/browser-kit": "^6.1.0",
        "symfony/framework-bundle": "^6.1.0",
        "symfony/phpunit-bridge": "^6.1.0",
        "symfony/twig-bundle": "^6.1.0",
        "symfony/var-dumper": "^6.1.0",
        "symfony/webpack-encore-bundle": "^1.11",
        "phpunit/phpunit": "^10.1",
        "symfony/yaml": "^6.2"
    },
    "conflict": {
        "survos/barcode-bundle": "<1.2.57",
        "survos/workflow-helper-bundle": "<1.2.57",
        "survos/import-bridge": "<1.2.57",
        "survos/providence-bundle": "<1.2.57",
        "survos/crawler-bundle": "<1.2.57",
        "survos/ruler-bundle": "<1.2.57",
        "survos/tree-bundle": "<1.2.57",
        "survos/stripe-product": "<1.2.57",
        "survos/inspection-bundle": "<1.2.57",
        "survos/doc-bundle": "<1.2.57",
        "survos/core-bundle": "<1.2.57",
        "survos/faker-bundle": "<1.2.57",
        "survos/bootstrap-bundle": "<1.2.57",
        "survos/auth-bundle": "<1.2.57",
        "survos/html-prettify-bundle": "<1.2.57",
        "survos/command-bundle": "<1.2.57",
        "survos/grid-bundle": "<1.2.57",
        "survos/maker-bundle": "<1.2.57",
        "survos/location-bundle": "<1.2.57",
        "survos/wiki-bundle": "<1.2.57",
        "survos/api-grid-bundle": "<1.2.57"
    }
}
